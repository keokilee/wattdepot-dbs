package org.wattdepot.server.db;

/**
 * An exception that is thrown when the database system encounters an extraordinary condition.
 * 
 * @author Robert Brewer
 */
public class DbException extends Exception {

  /**
   * The serialization UID, not that we'll ever use it. 
   */
  private static final long serialVersionUID = -1736619314071913949L;

  /**
   * Creates an Exception with the given message.
   *  
   * @param message the error message to include in the Exception.
   */
  public DbException(String message) {
    super(message);
  }
}
