package org.wattdepot.datainput;

/**
 * Allows RowParsers to indicate when there are problems parsing a row.
 * 
 * @author Robert Brewer
 */
public class RowParseException extends Exception {

  /** Keep Eclipse happy. */
  private static final long serialVersionUID = 1L;

  /**
   * Thrown when there are problems parsing a row.
   */
  public RowParseException() {
    super();
  }

  /**
   * Thrown when there are problems parsing a row.
   * 
   * @param message A message indicating the cause of the problem.
   */
  public RowParseException(String message) {
    super(message);
  }

  /**
   * Thrown when there are problems parsing a row.
   * 
   * @param cause The cause of the problem.
   */
  public RowParseException(Throwable cause) {
    super(cause);
  }

  /**
   * Thrown when there are problems parsing a row.
   * 
   * @param message A message indicating the cause of the problem.
   * @param cause The cause of the problem.
   */
  public RowParseException(String message, Throwable cause) {
    super(message, cause);
  }
}
