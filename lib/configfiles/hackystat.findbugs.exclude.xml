<!-- Used by findbugs.build.xml to exclude various kinds of false positives. -->
<FindBugsFilter>
  <!-- Match all occurrences of these bugcodes throughout the system. -->
  <Match>
    <Bug pattern="OBL_UNSATISFIED_OBLIGATION,J2EE,MS_PKGPROTECT" />
  </Match>

  <!-- Never check the autogenerated jaxb classes. -->
  <Match>
    <Package name="~.*\.jaxb" />
  </Match>

  <!-- Don't check autogenerated telemetry classes. -->
  <Match>
    <Or>
      <Package name="org.hackystat.telemetry.analyzer.language.ast" />
      <Package name="org.hackystat.telemetry.analyzer.language.parser" />
      <Package name="org.hackystat.telemetry.analyzer.language.parser.impl" />
    </Or>
  </Match>

  <!-- Don't worry about these SQL warnings in DerbyImplementation; they're OK. -->
  <Match>
    <Class name="org.hackystat.sensorbase.db.derby.DerbyImplementation" />
    <Method name="getProjectSummary"/>
    <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING"/>
  </Match>
  <Match>
    <Class name="org.hackystat.sensorbase.db.derby.DerbyImplementation" />
    <Method name="hasSensorData"/>
    <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING"/>
  </Match>

  <!-- Assume that the postgres guys know what they're doing. -->
  <Match>
    <Class name="org.hackystat.sensorbase.db.postgres.PostgresImplementation" />
    <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING"/>
  </Match>

</FindBugsFilter>
